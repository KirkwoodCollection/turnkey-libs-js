{
  "name": "@turnkey/auth-config",
  "version": "2.0.0",
  "description": "Centralized authentication and authorization configuration for TurnkeyHMS",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./headers": {
      "import": "./dist/headers.js",
      "require": "./dist/headers.js",
      "types": "./dist/headers.d.ts"
    },
    "./cors": {
      "import": "./dist/cors.js",
      "require": "./dist/cors.js",
      "types": "./dist/cors.d.ts"
    },
    "./jwt": {
      "import": "./dist/jwt.js",
      "require": "./dist/jwt.js",
      "types": "./dist/jwt.d.ts"
    },
    "./api-keys": {
      "import": "./dist/api-keys.js",
      "require": "./dist/api-keys.js",
      "types": "./dist/api-keys.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "jest": "^29.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0"
  },
  "keywords": [
    "turnkey",
    "hms",
    "auth",
    "authentication",
    "authorization",
    "jwt",
    "cors",
    "headers",
    "security"
  ],
  "author": "TurnkeyHMS Team",
  "license": "MIT",
  "sideEffects": false
}